{
    "name": "cloudshipco/enhanced-analytics",
    "description": "A powerful, efficient analytics solution for Statamic with real-time tracking, caching, and comprehensive dashboard visualization. Forked from mohammedshuaau/enhanced-analytics.",
    "type": "statamic-addon",
    "keywords": [
        "statamic",
        "statamic-addon",
        "analytics",
        "enhanced-analytics",
        "enhanced",
        "real-time",
        "tracking",
        "cache",
        "performance",
        "dashboard",
        "chart",
        "chartjs",
        "alpinejs"
    ],
    "autoload": {
        "psr-4": {
            "Mohammedshuaau\\EnhancedAnalytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mohammedshuaau\\EnhancedAnalytics\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Enhanced Analytics",
            "description": "A powerful, efficient analytics solution for Statamic"
        },
        "laravel": {
            "providers": [
                "Mohammedshuaau\\EnhancedAnalytics\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.3",
        "statamic/cms": "^5.0",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
